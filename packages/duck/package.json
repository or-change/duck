{
	"name": "@produck/duck",
	"version": "1.1.1",
	"description": "An extensible, lightweight, flexible, progressive software development framework for product.",
	"keywords": [
		"product",
		"framework",
		"IoC",
		"DI"
	],
	"author": "ChaosLee <chaos2lee@qq.com>",
	"homepage": "https://github.com/produck/duck#readme",
	"license": "MIT",
	"main": "src/index.cjs",
	"type": "module",
	"exports": {
		"types": "./index.d.ts",
		"import": "./src/index.mjs",
		"require": "./src/index.cjs"
	},
	"types": "index.d.ts",
	"files": [
		"src",
		"index.d.ts",
		"tsconfig.json"
	],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/produck/duck.git"
	},
	"scripts": {
