{
	"name": "@produck/duck",
	"version": "1.0.0",
	"description": "An extensible, lightweight, flexible, progressive software development framework for product.",
	"keywords": [
		"product",
		"framework",
		"IoC",
		"DI"
	],
	"author": "ChaosLee <chaos2lee@qq.com>",
	"homepage": "https://github.com/produck/duck#readme",
	"license": "MIT",
	"main": "index.mjs",
	"type": "module",
	"exports": {
		"import": "./src/index.mjs",
		"require": "./dist/duck.cjs"
	},
	"types": "index.d.ts",
	"files": [
		"src/**.mjs",
		"dist/duck.cjs",
		"index.d.ts",
		"tsconfig.json"
	],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/produck/duck.git"
	},
	"scripts": {
		"build:version": "node script/version.mjs",
		"build": "npm run build:version && rollup -c script/rollup.config.mjs",
		"prepublishOnly": " npm run build"
	},
	"bugs": {
		"url": "https://github.com/produck/duck/issues"
	},
	"dependencies": {
		"@produck/kit": "^0.1.2",
		"@produck/mold": "^0.2.2",
		"semver": "^7.3.7"
	},
	"devDependencies": {
		"rollup": "^2.79.1"
	}
}
