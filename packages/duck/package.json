		"build": "npm run build:extract && rollup -c script/rollup.config.mjs",
		"prepublishOnly": " npm run build"
	},
	"bugs": {
		"url": "https://github.com/produck/duck/issues"
	},
	"dependencies": {
		"@produck/kit": "^0.1.4",
		"@produck/mold": "^0.3.0",
		"semver": "^7.3.8"
	},
	"devDependencies": {
		"rollup": "^3.4.0"
	}
}
