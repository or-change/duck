{
  "packages": [
    "packages/*"
  ],
  "command": {
    "run": {
      "npmClient": "npm"
    },
    "bootstrap": {
      "npmClientArgs": [
        "--no-package-lock"
      ]
    },
    "publish": {
      "ignoreChanges": [
        "duck-log*",
        "duck-web-koa-validator"
      ]
    }
  },
  "version": "independent"
}